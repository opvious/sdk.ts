{
  "name": "@opvious/template-server",
  "version": "0.1.0",
  "repository": "github:opvious/template.ts",
  "description": "",
  "keywords": [],
  "author": "Opvious engineering <eng@opvious.ai>",
  "license": "UNLICENSED",
  "main": "lib/",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "prepare": "pnpm run build",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --testTimeout=1000000",
    "watch": "tsc -b -w src test"
  },
  "dependencies": {
    "@opvious/stl": "^0.5.28"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.7",
    "jest": "^27.4.5",
    "jest-extended": "^1.1.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.1.5"
  },
  "jest": {
    "coverageDirectory": "../../out/coverage/template-server",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    },
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ]
  },
  "typedocMain": "src/index.ts"
}
