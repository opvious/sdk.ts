{
  "name": "opvious",
  "version": "0.3.6",
  "repository": "github:opvious/sdk.ts",
  "description": "Opvious SDK",
  "homepage": "https://www.opvious.io/sdk.ts/modules/opvious.html",
  "keywords": [
    "opvious",
    "sdk"
  ],
  "author": "Opvious Engineering <oss@opvious.io>",
  "license": "Apache-2.0",
  "files": [
    "lib/"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./api": "./lib/api.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "fix": "prettier --write",
    "prepare": "pnpm run build",
    "test": "jest",
    "watch": "tsc -b -w src"
  },
  "dependencies": {
    "@opentelemetry/api": "~1.3.0",
    "@opvious/api": "^0.13.5",
    "@opvious/stl-errors": "^0.15.15",
    "@opvious/stl-telemetry": "^0.15.15",
    "backoff": "^2.5.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@types/backoff": "^2.5.2",
    "@types/node-fetch": "^2.6.2",
    "node-fetch": "^2.6.7"
  },
  "jest": {
    "coverageDirectory": "../../out/coverage/sdk",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "babelConfig": {
            "compact": false
          },
          "tsconfig": "test/tsconfig.json"
        }
      ]
    }
  },
  "typedoc": {
    "entryPoint": "src/index.ts"
  }
}
