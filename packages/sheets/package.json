{
  "name": "opvious-sheets",
  "version": "0.3.8",
  "repository": "github:opvious/sdk.ts",
  "description": "Opvious spreadsheet utilities",
  "homepage": "https://www.opvious.io/sdk.ts/modules/opvious_sheets.html",
  "keywords": [
    "opvious"
  ],
  "author": "Opvious Engineering <oss@opvious.io>",
  "license": "Apache-2.0",
  "main": "./lib",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "prepare": "pnpm run build",
    "test": "jest",
    "watch": "tsc -b -w src"
  },
  "dependencies": {
    "@opvious/api": "^0.13.9",
    "@opvious/stl-errors": "^0.15.15",
    "@opvious/stl-utils": "^0.15.15",
    "change-case": "^4.1.2",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/pluralize": "^0.0.29"
  },
  "jest": {
    "coverageDirectory": "../../out/coverage/sheets",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "babelConfig": {
            "compact": false
          },
          "tsconfig": "test/tsconfig.json"
        }
      ]
    }
  },
  "typedoc": {
    "entryPoint": "src/index.ts"
  }
}
