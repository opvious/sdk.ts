{
  "name": "opvious-sheets",
  "version": "0.1.35",
  "repository": "github:opvious/sdk.ts",
  "description": "Opvious spreadsheet utilities",
  "homepage": "https://www.opvious.io",
  "keywords": [
    "opvious"
  ],
  "author": "Opvious Engineering <oss@opvious.io>",
  "license": "Apache-2.0",
  "main": "./lib",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "prepare": "pnpm run build",
    "test": "jest",
    "watch": "tsc -b -w src"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "opvious-graph": "workspace:*",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^18.11.7",
    "@types/pluralize": "^0.0.29",
    "jest": "^27.4.5",
    "jest-extended": "^1.1.0",
    "ts-essentials": "^9.3.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.4"
  },
  "jest": {
    "coverageDirectory": "../../out/coverage/sheets",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    },
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "transform": {}
  },
  "typedoc": {
    "entryPoint": "src/index.ts"
  }
}
