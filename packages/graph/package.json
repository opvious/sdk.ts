{
  "name": "opvious-graph",
  "version": "0.1.28",
  "repository": "github:opvious/sdk.ts",
  "description": "Opvious GraphQL API",
  "homepage": "https://www.opvious.io",
  "keywords": [
    "opvious"
  ],
  "author": "Opvious <oss@opvious.io>",
  "license": "Apache-2.0",
  "main": "lib/",
  "files": [
    "lib/",
    "resources/"
  ],
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out $(find src -name '*.gen.ts')",
    "gen": "./scripts/generate-api-types",
    "prepare": "pnpm run gen && pnpm run build",
    "watch": "tsc -b -w src"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-generic-sdk": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@opvious/api-graph": "^0.7.12",
    "@types/node": "^18.8.5",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "typedoc": {
    "entryPoint": "src/index.ts"
  }
}
